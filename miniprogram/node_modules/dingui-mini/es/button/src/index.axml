<view a:if="{{onDmCatchTap}}" catchTap="onDmCatchTap" class="dm-button {{className}}  dm-button-{{type}} dm-button-size-{{size}} {{disabled?'dm-button-disable':''}} {{loading?'dm-button-loading':''}}">
  <button type="default"  class="dm-button-button" open-type="{{openType}}" form-type="{{formType}}" disabled="{{disabled}}">
    <dingui-icon type="loading" className="dm-button-loading-icon" a:if="{{loading}}"/>
    <slot></slot>
  </button>
</view>
<view onTap="onDmTap" a:else class="dm-button {{className}}  dm-button-{{type}} dm-button-size-{{size}} {{disabled?'dm-button-disable':''}} {{loading?'dm-button-loading':''}}">
   <button type="default"  class="dm-button-button" open-type="{{openType}}" form-type="{{formType}}" disabled="{{disabled}}">
    <dingui-icon type="loading" className="dm-button-loading-icon" a:if="{{loading}}"/>
    <slot></slot>
  </button>
</view>