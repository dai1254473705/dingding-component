.dm-list-font-elipsis {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.dm-list-item {
  white-space: pre-line;
  word-break: break-all;
  background-color: #ffffff;
}
.dm-list-item:last-of-type .dm-list-item-content .dm-list-item-main .dm-list-item-border {
  background-color: transparent;
}
.dm-list-item:last-of-type .dm-list-item-content .dm-list-item-main .dm-list-item-border-hasborder {
  background-color: rgba(25, 31, 37, 0.12);
}
.dm-list-item .dm-list-item-content {
  position: relative;
  display: flex;
  align-items: center;
  box-sizing: border-box;
  padding-left: 32rpx;
}
.dm-list-item-thumb-wrapper {
  margin-right: 32rpx;
}
.dm-list-item-thumb-valign-top {
  align-self: flex-start;
  margin-top: 32rpx;
}
.dm-list-item-thumb-size-mini {
  width: 48rpx;
  height: 48rpx;
  font-size: 16rpx;
  line-height: 48rpx;
}
.dm-list-item-thumb {
  display: flex;
  flex-direction: row;
  background-repeat: no-repeat;
  background-position: center;
  background-size: 100% 100%;
  background-size: cover;
}
.dm-list-item-thumb-default {
  background-color: #add5fd;
}
.dm-list-item-thumb-size-standard {
  width: 72rpx;
  height: 72rpx;
  font-size: 28rpx;
  line-height: 38rpx;
  line-height: 72rpx;
}
.dm-list-item-thumb-size-big {
  width: 96rpx;
  height: 96rpx;
  font-size: 28rpx;
  line-height: 38rpx;
  line-height: 96rpx;
}
.dm-list-item-thumb-size-large {
  width: 112rpx;
  height: 112rpx;
  font-size: 32rpx;
  line-height: 42rpx;
  line-height: 112rpx;
}
.dm-list-item-thumb-type-square {
  overflow: hidden;
  border-radius: 6rpx;
}
.dm-list-item-thumb-type-circle {
  overflow: hidden;
  border-radius: 50%;
}
.dm-list-item-content {
  position: relative;
  display: flex;
  flex-direction: row;
  align-items: center;
  box-sizing: border-box;
  padding-left: 32rpx;
}
.dm-list-item-main {
  position: relative;
  display: flex;
  flex: 1;
  flex-direction: row;
  align-items: center;
  box-sizing: border-box;
  min-height: 112rpx;
  padding-right: 32rpx;
  overflow: hidden;
}
.dm-list-item-large-main {
  min-height: 176rpx;
}
.dm-list-item-caption {
  color: #191f25;
  font-size: 34rpx;
  line-height: 44rpx;
  letter-spacing: -0.82rpx;
}
.dm-list-item-border {
  position: absolute;
  right: 0;
  bottom: 1rpx;
  left: 0;
  height: 1rpx;
  background-color: rgba(25, 31, 37, 0.12);
}
.dm-list-item-border-hasborder {
  background-color: rgba(25, 31, 37, 0.12);
}
.dm-list-item-brief-item {
  font-size: 26rpx;
  line-height: 36rpx;
  margin-top: 8rpx;
  color: rgba(25, 31, 37, 0.56);
  letter-spacing: -0.16rpx;
}
.dm-list-item-brief-no-margin {
  margin-top: 0;
}
.dm-list-item-main-content {
  display: flex;
  flex: 1;
  flex-direction: column;
  height: 100%;
  padding-top: 32rpx;
  padding-bottom: 32rpx;
}
.dm-list-item-extra {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  font-size: 26rpx;
  line-height: 36rpx;
  display: flex;
  flex-direction: row;
  align-content: flex-end;
  align-items: center;
  max-width: 208rpx;
  color: rgba(25, 31, 37, 0.56);
  letter-spacing: -0.16rpx;
  padding-left: 32rpx;
}
.dm-list-item-extra-valign-top {
  align-self: flex-start;
  margin-top: 32rpx;
}
.dm-list-item-badge {
  display: flex;
  align-items: center;
  margin-left: 8rpx;
}
.dm-list-item-wrapper-arrow {
  font-size: 32rpx;
  line-height: 42rpx;
  margin-right: -32rpx;
  padding-right: 20rpx;
  padding-left: 8rpx;
  line-height: 112rpx;
  text-align: right;
}
.dm-list-item-wrapper-arrow-color-blue {
  color: rgba(25, 31, 37, 0.4);
}
.dm-list-item-wrapper-arrow-color-gray {
  color: rgba(25, 31, 37, 0.4);
}
.dm-list-item-checkbox-wrapper {
  width: 44rpx;
  height: 44rpx;
  margin-right: 24rpx;
  line-height: 44rpx;
  text-align: center;
}
.dm-list-item-disabled {
  color: rgba(25, 31, 37, 0.28);
}
.dm-list-item-hover:active {
  background-color: rgba(25, 31, 37, 0.08);
}
.dm-list-item-thumb-content {
  color: #ffffff;
  background-color: #3296fa;
  justify-content: center;
}
