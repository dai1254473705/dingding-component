<view class="dm-list {{className}}" onTap="onDmTap">
  <view a:if="{{$slots.header}}" class="dm-list-header">
    <slot name="header" />
  </view>
  <view a:if="{{$slots.header2}}" class="dm-list-header2">
    <slot name="header2" />
  </view>
  <view class="dm-list-body">
    <slot />
  </view>
  <view a:if="{{$slots.footer}}" class="dm-list-footer">
    <slot name="footer" />
  </view>
</view>