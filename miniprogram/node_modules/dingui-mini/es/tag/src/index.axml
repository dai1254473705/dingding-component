<view a:if="{{!closed}}" class="dm-tag-{{type}} dm-tag-{{closebable?'large':size}} dm-tag {{className}}  {{checked?`dm-tag-${type}-hover`:''}} {{closebable||size=='large'||!autoChecked?`dm-tag-${type}-hashover`:''}}" onTap="onDmTap">
  <text><slot>{{text}}</slot></text>
  <block a:if="{{closable}}">
    <dingui-icon type="close" onDmTap="onDmClose" className="dm-tag-close" />
  </block>
</view>