<view class="dm-empty {{className}} {{inpage?'dm-empty-inpage':''}}">
  <view class="dm-empty-content">
    <image class="dm-empty-img" src="{{img}}" mode="widthFix" a:if="{{img}}"/>
    <slot name="topIconSlot" a:if="{{!img}}">
      <dingui-icon className="dm-empty-icon" type="{{iconType}}"></dingui-icon>
    </slot>
    <view class="dm-empty-title" a:if="{{title}}">{{title}}</view>
    <view class="dm-empty-describe" a:if="{{describe && describe.indexOf('<br/>') == -1}}">
      {{describe}}
    </view>
    <view class="dm-empty-describe" a:if="{{describeArr.length}}">
      <view class="dm-empty-describe-wrap" a:for="{{describeArr}}">{{item}}</view>
    </view>
    <view class="dm-empty-link" a:if="{{linkText}}" onTap="onDmEmptyLinkTap">{{linkText}}</view>
    <dingui-button className="dm-empty-btn" type="primary" onDmTap="onDmEmptyBtnTap" a:if="{{btnName}}">{{btnName}}</dingui-button>
  </view>
</view>