.dm-empty {
  position: fixed;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  display: flex;
  align-items: center;
  justify-content: center;
  padding: 80rpx;
  text-align: center;
  background: #f6f6f6;
}
.dm-empty-content {
  max-width: 590rpx;
  margin-top: -200rpx;
  word-break: break-word;
}
.dm-empty-inpage {
  position: relative;
}
.dm-empty-inpage .dm-empty-content {
  margin-top: 0;
}
.dm-empty-icon {
  display: inline-block;
  width: 352rpx;
  color: rgba(25, 31, 37, 0.4);
  font-size: 160rpx;
  text-align: center;
}
.dm-empty-img {
  width: 352rpx;
}
.dm-empty-title {
  font-size: 34rpx;
  line-height: 44rpx;
  margin-top: 32rpx;
  color: #191f25;
}
.dm-empty-describe {
  font-size: 28rpx;
  line-height: 38rpx;
  margin-top: 16rpx;
  color: rgba(25, 31, 37, 0.4);
}
.dm-empty-link {
  font-size: 28rpx;
  line-height: 38rpx;
  color: #3296fa;
}
.dm-empty-btn {
  min-width: 274rpx;
  max-width: 590rpx;
  margin-top: 46rpx;
}
.dm-empty-describe-wrap {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
