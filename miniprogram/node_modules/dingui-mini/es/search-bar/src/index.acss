.dm-search-bar {
  padding: 16rpx 32rpx;
  background: #ffffff;
  border-bottom: 1rpx solid rgba(25, 31, 37, 0.12);
}
.dm-search-bar-wrap {
  display: flex;
  flex: 1;
  flex-direction: row;
  align-items: center;
  height: 72rpx;
  background: rgba(25, 31, 37, 0.12);
  border-radius: 6rpx;
}
.dm-search-bar-input {
  flex: 1;
  height: 40rpx;
  padding: 0;
  color: #191f25;
  font-size: 30rpx;
  background: none;
}
.dm-search-bar-input-disabled {
  color: rgba(25, 31, 37, 0.4);
}
.dm-search-bar-icon {
  width: 72rpx;
  height: 40rpx;
  color: rgba(25, 31, 37, 0.4);
  line-height: 40rpx;
  text-align: center;
}
