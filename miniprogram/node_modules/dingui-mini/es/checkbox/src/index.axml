<view class="dm-checkbox {{className}}">
  <view class="dm-checkbox-wrap">
    <dingui-icon className="dm-checkbox-icon {{isChecked?'':'dm-checkbox-icon-checkbox'}} {{disabled && isChecked? 'dm-checkbox-fill-disabled':''}} {{disabled &&  !isChecked? 'dm-checkbox-icon-disabled':''}}" type="{{isChecked?'checkbox-fill':'checkbox'}}" size="{{size}}"  onDmTap="onDmChange"></dingui-icon>
  </view>
</view>