<view class="dm-checkbox-item {{className}}">
   <dingui-list-item
      className="dm-checkbox-item-list"
      disabled="{{disabled}}"
      hasborder="{{true}}" 
      arrowType="{{hasArrowIcon?arrowType:''}}"
      arrowSize="{{hasArrowIcon?arrowSize: ''}}"
      index="{{index}}"
      checkable="{{true}}"
      checked="{{isChecked}}"
      onDmArrowClick="onDmExtraClick"
      onDmChange="onDmChange"
    >
      {{itemName}}
  </dingui-list-item>
</view>