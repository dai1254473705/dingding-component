.dm-checkbox-item {
  background: #ffffff;
}
.dm-checkbox-item:last-of-type .dm-list-item:last-of-type .dm-list-item-content .dm-list-item-border {
  background-color: transparent;
}
.dm-checkbox-icon-wrap {
  display: flex;
}
.dm-checkbox-item-check-icon {
  margin-right: 24rpx;
  color: #3296fa;
}
.dm-checkbox-item-check-icon-show {
  opacity: 1;
}
.dm-checkbox-item-check-icon-hide {
  opacity: 0;
}
