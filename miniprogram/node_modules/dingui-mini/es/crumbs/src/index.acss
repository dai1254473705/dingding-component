.dm-crumbs {
  display: flex;
  flex-direction: row;
  width: 100%;
  height: 96rpx;
  overflow: auto;
  overflow-y: hidden;
  line-height: 96rpx;
  white-space: nowrap;
  background-color: #ffffff;
  border-bottom-color: rgba(25, 31, 37, 0.12);
  border-bottom-width: 1rpx ;
  border-bottom-style: solid;
}
.dm-crumbs-item {
  display: inline-block;
  display: flex;
  align-items: center;
  justify-content: center;
  max-width: 544rpx;
  padding-right: 16rpx;
  padding-left: 16rpx;
  overflow: hidden;
  color: #3296fa;
  font-size: 30rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  vertical-align: middle;
}
.dm-crumbs-item-first-child {
  padding-left: 32rpx;
}
.dm-crumbs-item-last-child {
  padding-right: 32rpx;
}
.dm-crumbs-silent {
  color: rgba(25, 31, 37, 0.56);
}
.dm-crumbs-arrow {
  display: flex;
  align-items: center;
  justify-content: center;
  color: rgba(25, 31, 37, 0.4);
}
